/* box-pane (orange)*/
.box-pane {
	width:300px;
	height:100%;
	float:left;
	margin: 0 .8em 1em .8em;
	background : url(../img/yellow-orange-top.png) no-repeat;
}

.box-pane h2 { width: 300px; height : 44px; padding:10px 0 0 0; text-align:center; margin:0;  color: #fff; }

.box-pane-content {
	padding : .5em 0 1em 0; 
	background : url(../img/body-box.png) no-repeat 0 100%;  
	margin : -1em 0 0 0; 
	font-size:1.2em;
}

.box-pane-content a {
	text-decoration: none;
}

.box-pane-content ul {
	list-style: none;
	padding:0;
	margin-left: 1em;
	
}

.box-pane-content p {
	margin-left:1em;
	margin-right:.5em;
}

.box-pane-content div {
	width:140px;
	float:left;
}

.checked li a { padding-left : 1.5em; background : url(../img/request_replied.png) no-repeat 0 50%; }


/*= outer tabs */
ol#outer-tabs {
	height: 2em;
	list-style: none;
	margin: 0;
	padding: 0;
	padding-left:45px;
	background:#ccc;
}
ol#outer-tabs a {
	background: #bdf url(../img/tabs.png);
	color: #705a8c;
	display: block;
	float: left;
	height: 2em;
	padding-left: 10px;
	text-decoration: none;
	position:relative;
	top:2px;
	font-weight:bold;
}
ol#outer-tabs a:hover {
	background-color: #3af;
	background-position: 0 -120px;
}
ol#outer-tabs a:hover span {
	background-position: 100% -120px;
}

ol#outer-tabs li, ol#target_list li, ol.checklist-horizontal li {
	float: left;
	margin: 0 2px 0 0;
}

ol#outer-tabs li.current a {
	background-color: #48f;
	background-position: 0 -60px;
	color: #e66313;
	font-weight: bold;
	top:2px;
}
ol#outer-tabs li.current span {
	background-position: 100% -60px;
}
ol#outer-tabs span {
	background: url(../img/tabs.png) 100% 0;
	display: block;
	line-height: 2em;
	padding-right: 10px;
}


/* START: Popup tabs */
ol#popup_outer_tabs {
	height: 2em;
	list-style: none;
	margin: 0;
	padding: 0;
	padding-left:15px;
	background:#fff;
}
ol#popup_outer_tabs a {
	background: url(../img/popup_tabs_1.png);
	color: #705a8c;
	display: block;
	float: left;
	padding-left: 10px;
	text-decoration: none;
	position:relative;
	font-weight:bold;
	height: 2em;
	top:4px;
}
ol#popup_outer_tabs a:hover {
	background-color: #fff;
	background-position: 0 -124px;
}

ol#popup_outer_tabs a:hover span {
	background-position: 100% -124px;
}

ol#popup_outer_tabs li{
	float: left;
	margin: 0 2px 0 0;
}
ol#popup_outer_tabs li.popup-current a {
	background-color: #fff;
	background-position: 0 -62px;
	color: #e66313;
	font-weight: bold;
	height: 23px;
	top:4px;
}
ol#popup_outer_tabs li.popup-current span {
	background-position: 100% -62px;
}

ol#popup_outer_tabs span {
	background: url(../img/popup_tabs_1.png) 100% 0;
	display: block;
	line-height: 2em;
	padding-right: 10px;
	height: 23px;
	top:4px;
}
/* END: Popup tabs */

/*= inner tabs */
