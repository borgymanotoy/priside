/* = top menu */
 ul#menu {
  font-family: Arial, Verdana;
  font-size: .9em;
  font-weight:bold;
  margin: 0 0 0 420px;
  padding: 0;
  list-style: none;
}
ul#menu li {
  display: block;
  position: relative;
  float: left;
}
ul#menu li ul { display: none; }
ul#menu li a {
  display: block;
  text-decoration: none;
  padding: 0 4px 0 4px;
  background: #fff;
  white-space: nowrap;
}
ul#menu li a:hover { color:#000;}
ul#menu li:hover ul {
  display: block;
  position: absolute;
  padding:0;
  margin:0;
}
ul#menu li:hover li {
  float: none;
}
ul#menu li:hover a { color: #E66313; }
ul#menu li:hover li a { color: #000; }
ul#menu li:hover li a:hover { background:#E66313; color: #000;  }
.menu-selected:hover {
	color: #E66313;
	background:fff;
}
#menu .menu-selected {
	background:#E66313; 
	color:#fff;
}

/*=sub menu generic*/
.submenu li {
	display:inline;
}
.submenu li a {
	text-decoration:none; padding: 0 4px 0 4px;
}
.submenu li a:hover {
	color:#E66313;
}
a.submenu-current, a.pages_current, a.hiw_current, a.faq_current, a.cu_current, a.mycp-current, a.inquiries_current, a.myproject_current, a.myaccount_current
{
	color:#E66313;
}
ul.submenu {
	padding: 0px;
	margin: 0px;
}
