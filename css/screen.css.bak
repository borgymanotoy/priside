	height:41px;
	width: 100%;
	margin: 0 auto; 
	background: url('../img/main_bottom.png') no-repeat;
}

/*=footer*/
#footer{
	height: 100%;
	width: 100%; 
	margin-top:2em;
	color:#ffffff;
	font-weight:bold;
}

#content-footer {
	width: 1000px;
	background: url('../img/footer_body.png') repeat-y;
}

#main-content-footer {
	height:100%;
	width: 100%;
	background: url('../img/footer_top.png') no-repeat;
	width: 980px;
	padding:0 5px 0 15px;
}

#main-bottom-footer {
	height:29px;
	width: 100%;
	margin: 0 auto; 
	background: url('../img/footer_bottom.png') no-repeat;
}

ul#company-info { 
	list-style-type : none;
	padding : 1em;	
	margin-left:2em;
}

ul#company-info:after { 
	content : ".";
	display : block;
	height : 0;
	clear : both;
	visibility : hidden; 
}

ul#company-info li { 
	float : left; 
	width : 230px; 
}

ul#company-info ul { 
	list-style-type : none; 
	padding : 0; 
}

#company-info h3 {
	color:#f7c001;
	margin : 0;
	padding : 0;
	font-size : 100%;
}
#main-content-footer p {
	text-align:center;
}

/*=link*/
#company-info a:link { color : #fff; text-decoration : none; }
#company-info a:visited { color : #fff; text-decoration : none; }
#company-info a:hover { color : #fff; text-decoration : underline; }
#company-info a:active, #company-info a:focus { color : #fff; }


