/* START: Search list box */
.dropbox{
	width: 250px;
	color : #000;
	border:2px solid #412467;
	padding:2px 5px;
	margin-left: 0px;
	margin-bottom:0px;
	-moz-border-radius: 8px;
	-webkit-border-radius: 8px;
	-khtml-border-radius: 8px;
	border-radius: 8px;
	vertical-align: middle;
	position: relative;
	display: inline-block;
}
.dropboxLabel {
	height: 18px;
	width: 200px;
	padding-left: 0px; /* padding-left: 10px; */
	margin-left: 10px;
	display: block;
}
.listBox {
	width: 200px;
	position: absolute;
	left: -3px;
	top: 27px;
	padding: 0px;
	color: #412467;
	background-color: #f4f4f6;
	border:2px solid #e7e9e9;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	-khtml-border-radius: 5px;
	border-radius: 5px;
	box-shadow: 2px 2px 1px #888888;
}
.search-input-box { 
	border: 0px; 
	width: 175px;
	padding-left: 5px;
}
/* END: Search list box */



