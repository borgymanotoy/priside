/*= service request table */
.requestsHeader {
   display: block;
   margin: 0px;
   padding: 0px;
   line-height: 100%;
   color: #ffffff;
   background-color: #412567;
   text-transform:uppercase ;
}


/* Wraps each service request. */
.serviceRequest {
   display: block;
   padding-right: 0px;
   margin-top: 10px;
   vertical-align: top;
   color: #222222;
   background-color: #f6f4f8 ;
}

.serviceRequest.even { background:#d9d3e1}
.serviceRequest.odd { }

/* ====== START: Box wrapping service table header ====== */
.serviceheadercontainer_top_left {
   background: url("../img/roundbox_border_lightblue.png") top left no-repeat;
}

.serviceheadercontainer_bottom_right {
   background: url("../img/roundbox_border_lightblue.png") bottom right no-repeat;
}

.serviceheadercontainer_top_right {
   background: url("../img/roundbox_border_lightblue_tr.png") top right no-repeat;
}

/* If paddings inside the box are desired they shall be set in this class. */
.serviceheadercontainer_bottom_left {
   padding: 1px;
   padding-top: 5px;
   padding-bottom: 5px;
   background: url("../img/roundbox_border_lightblue_bl.png") bottom left no-repeat;
}
/* ====== END:   Box wrapping service table header ====== */

/* ====== START: Box wrapping each service row ====== */

.servicecontainer_top_left, .textarea_top_left{
   background: url("../img/roundbox_border_lightblue.png") top left no-repeat;
}

.servicecontainer_top_left:hover {
   background: url("../img/roundbox_border.png") top left no-repeat;
}

.servicecontainer_bottom_right, .servicecontainer-box-top-br, .textarea_bottom_right  {
   background: url("../img/roundbox_border_lightblue.png") bottom right no-repeat;
}
.servicecontainer_bottom_right:hover {
   background: url("../img/roundbox_border.png") bottom right no-repeat;
}
.servicecontainer_top_right, .textarea_top_right {
   background: url("../img/roundbox_border_lightblue_tr.png") top right no-repeat;
}
.servicecontainer_top_right:hover {
   background: url("../img/roundbox_border_tr.png") top right no-repeat;
}

/* If paddings inside the box are desired they shall be set in this class. */
.servicecontainer_bottom_left, .servicecontainer-box-top-bl, .textarea_bottom_left {

   background: url("../img/roundbox_border_lightblue_bl.png") bottom left no-repeat;
}
.servicecontainer_bottom_left:hover {
   background: url("../img/roundbox_border_bl.png") bottom left no-repeat;
}

/* ====== END:   Box wrapping each service row ====== */


.servicecontainer-box-top {
   background: url("../img/box_border_top_lightblue.png") top left no-repeat;
}

